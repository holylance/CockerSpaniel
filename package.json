    "solium": "^1.2.5",
    "truffle-assertions": "0.9.2",
    "truffle-flattener": "1.4.4",
    "web3": "1.2.6",
    "web3-eth-abi": "1.3.0",
    "await-sleep": "0.0.1",
    "ethereum-input-data-decoder": "0.3.0",
